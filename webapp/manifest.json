{
  "_version": "1.76.0",
  "sap.app": {
    "id": "trainingconnections",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.3",
      "toolsId": "1ceb9b63-f3e5-4ca9-9be3-55369ea86dc3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zwb_connections_o4/srvd_a2x/sap/zconnections/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.139.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "trainingconnections.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "trainingconnections.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "ConnDetail",
          "pattern": "connection/{id}",
          "target": "ConnDetail"
        },
        {
          "name": "NewConnection",
          "pattern": "newConnection",
          "target": "NewConnection"
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "ConnDetail": {
          "id": "ConnectionDetail",
          "name": "connection.Connection"
        },
        "NewConnection": {
          "id": "NewConnection",
          "name": "CreateConnection"
        }
      }
    },
    "rootView": {
      "viewName": "trainingconnections.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
